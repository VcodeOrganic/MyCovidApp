<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#FFFFFF"
    android:orientation="vertical"
    android:padding="5dp">

    <androidx.cardview.widget.CardView
        android:id="@+id/matchCard12"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:padding="5dp"
        app:cardCornerRadius="5dp">

        <LinearLayout
            android:id="@+id/team_1_lin_layout2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/custom_border"
            android:orientation="vertical"
            android:padding="5dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:padding="5dp">

                <ImageView
                    android:id="@+id/imageView82"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"

                    android:layout_gravity="center"
                    android:layout_weight="4"
                    app:srcCompat="@drawable/doctor" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/docNameRecycler"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/abeezee"
                        android:padding="10dp"
                        android:text="Doctor Name"
                        android:textColor="#000000" />

                    <TextView
                        android:id="@+id/docInstiRecycler"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/abeezee"
                        android:padding="10dp"
                        android:text="Doctor Institute Name"
                        android:textColor="#000000" />

                    <TextView
                        android:id="@+id/docPhoneRecycler"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/abeezee"
                        android:padding="10dp"
                        android:text="Doctor Phone"
                        android:textColor="#000000" />

                    <TextView
                        android:id="@+id/docTimeRecycler"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/abeezee"
                        android:padding="10dp"
                        android:text="Doctor Time"
                        android:textColor="#000000" />

                    <!--                    <EditText-->
                    <!--                        android:id="@+id/etDocInsti2"-->
                    <!--                        android:layout_width="match_parent"-->
                    <!--                        android:layout_height="wrap_content"-->
                    <!--                        android:ems="10"-->
                    <!--                        android:hint="Enter InstiName"-->
                    <!--                        android:inputType="textPersonName"-->
                    <!--                        android:padding="10dp"-->
                    <!--                        android:textSize="14sp" />-->

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="4"
                    android:orientation="vertical">

                    <ImageButton
                        android:id="@+id/callBtn"
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_gravity="center"
                        android:layout_weight="4"
                        android:background="@drawable/custom_border_call"
                        app:srcCompat="@drawable/call_svg" />

                    <TextView
                        android:id="@+id/callText"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="4"
                        android:fontFamily="@font/abeezee"
                        android:gravity="center"
                        android:text="CALL NOW"
                        android:textColor="#4169E1"
                        android:textSize="14sp" />
                </LinearLayout>


            </LinearLayout>


        </LinearLayout>

    </androidx.cardview.widget.CardView>

</RelativeLayout>